{
  "name": "web",
  "version": "0.0.1",
  "description": "Web V3",
  "main": "index.js",
  "scripts": {
    "analyze": "ANALYZE=1 npm run build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix .",
    "test": "jest --watch",
    "format": "prettier --write \"{,!(node_modules)/**/}*.{ts,tsx}\"",
    "test:badges": "npm run test:coverage  && jest-coverage-badges --output ./badges",
    "test:coverage": "jest --colors --coverage --forceExit",
    "postinstall": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SapienNetwork/web.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "npm run test:badges",
      "prettier --write",
      "eslint --fix --max-warnings=0",
      "jest --forceExit --findRelatedTests --maxWorkers=2"
    ]
  },
  "bugs": {
    "url": "https://github.com/SapienNetwork/web/issues"
  },
  "homepage": "https://github.com/SapienNetwork/web#readme",
